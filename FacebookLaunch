package JavaPackage;
import org.openqa.selenium.WebDriver; 
import org.openqa.selenium.chrome.ChromeDriver;


public class OpenBrowser1 {
	//main class
	public static void main(String[] args) throws InterruptedException {
		//'webdriver.chrome.driver' is the property of Chrome Browser and path of driver 
		System.setProperty("webdriver.chrome.driver", "C:\\Automation Testing Software\\chromedriver_win32\\chromedriver.exe");
		
		//WebDriver is interface of Selenium & ChromeDriver is a Class of webdriver interface for chrome browser
		WebDriver driver = new ChromeDriver(); 

		//get is method to load new web pages
		driver.get("https://www.facebook.com/"); 

		// manage is method to manage windows and maximize is performing operation on windows
		driver.manage().window().maximize(); 
		

		
		
	}

}
